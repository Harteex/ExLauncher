<?xml version="1.0" encoding="utf-8"?>
<Screen title="">
  <FramePanel
    width="*"
    height="*">
    <Image
      width="content"
      height="content"
      path="@theme/appIconDefault.png"
      layoutMargin="5"/>
    <Image
      width="content"
      height="content"
      path="@theme/appIconDefault.png"
      layoutGravity="top|left"/>
    <Image
      width="content"
      height="content"
      path="@theme/appIconDefault.png"
      layoutGravity="bottom|right"/>
    <Image
      width="content"
      height="content"
      path="@theme/appIconDefault.png"
      layoutGravity="center_vertical|right"/>
    <Image
      width="content"
      height="content"
      path="@theme/appIconDefault.png"
      layoutGravity="bottom"/>
    <Image
      width="content"
      height="content"
      path="@theme/appIconDefault.png"
      layoutGravity="right"/>
    <Image
      width="content"
      height="content"
      path="@theme/appIconDefault.png"
      layoutGravity="center_horizontal"/>
    <Image
      width="content"
      height="content"
      path="@theme/appIconDefault.png"
      layoutGravity="center_horizontal|center_vertical"/>
    <Image
      width="content"
      height="content"
      path="@theme/appIconDefault.png"
      layoutGravity="center_vertical"/>
    <Image
      width="content"
      height="content"
      path="@theme/appIconDefault.png"
      layoutGravity="bottom|center_horizontal"/>
  </FramePanel>
</Screen>